{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^3.0",
        "drupal/bootstrap_barrio": "^5.1",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "^2.0@beta",
        "drupal/core-recommended": "^8.9.0",
        "drupal/crop": "^2.1",
        "drupal/default_content": "2.0.x-dev@dev",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/field_group": "^3.1",
        "drupal/focal_point": "^1.4",
        "drupal/genpass": "1.x-dev",
        "drupal/honeypot": "^2.0",
        "drupal/language_switcher_extended": "^1.0@beta",
        "drupal/linkit": "^6.0",
        "drupal/login_redirect_per_role": "^1.7",
        "drupal/maillog": "^1.0@beta",
        "drupal/maxlength": "^1.0",
        "drupal/media_bulk_upload": "^1.0",
        "drupal/media_library_edit": "^2.0",
        "drupal/menu_block": "^1.6",
        "drupal/menu_breadcrumb": "^1.14",
        "drupal/openid_connect": "^1.0",
        "drupal/openid_connect_harid": "^1.1",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/schema_metatag": "^2.1",
        "drupal/search_api": "^1.19",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/simple_recaptcha": "^1.0@beta",
        "drupal/svg_image": "^1.13",
        "drupal/taxonomy_menu_ui": "^1.4",
        "drupal/time_field": "^1.13",
        "drupal/token": "^1.7",
        "drupal/twig_tweak": "^2.6",
        "drupal/webform": "^6.0",
        "drush/drush": "^10.2",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/dropzonejs/composer.libraries.json",
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/core": {
                "Class active-trail not added to li element when linking to front page https://www.drupal.org/project/drupal/issues/1578832": "https://www.drupal.org/files/issues/2018-11-27/menu-class-active-trail-1578832-52.patch"
            },
            "drupal/media_library_edit": {
                "On fields with multiple media items, other items disappear after saving. https://www.drupal.org/project/media_library_edit/issues/3192521": "https://www.drupal.org/files/issues/2021-01-13/update_save_callback-3192521-2.patch"
            },
            "drupal/media_bulk_upload": {
                "Provide a way to add alt text when doing bulk upload [media_bulk_upload] https://www.drupal.org/project/media_bulk_upload/issues/3023825": "https://www.drupal.org/files/issues/2020-06-26/media_bulk_upload-3023825-25.patch"
            },
            "drupal/dropzonejs": {
                "Provide a way to add alt text when doing bulk upload [dropzonejs] https://www.drupal.org/project/media_bulk_upload/issues/3023825": "https://www.drupal.org/files/issues/2020-10-05/dropzonejs-3023825-27.patch"
            },
            "drupal/focal_point": {
                "Stylish the preview button according to the admin theme CSS https://www.drupal.org/project/focal_point/issues/3162961": "https://www.drupal.org/files/issues/2020-08-03/focal_point-stylish_preview_button-3162961-2.patch"
            },
            "drupal/conditional_fields": {
                "Notice: Trying to access array offset on value of type null https://www.drupal.org/project/conditional_fields/issues/3191984": "https://www.drupal.org/files/issues/2021-01-21/3191984-3-notice-trying-to-access-offset-on-value-of-type-null.patch"
            }
        }
    }
}
