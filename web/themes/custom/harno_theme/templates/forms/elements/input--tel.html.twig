{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}

  <div class="form-item custom-phone {% if element['#errors']%} error {% endif%} {% if element['#help'] %} help-text {% endif %}" data-plugin="movingLabel"
       {% if element['#help'] %}aria-label="{{ element['#help'] }}" {% endif %}>
    <label for="{{ element['#id'] }}" class="form-label">{{ element['#title'] }}
      {% if element['#required'] %}
        *
      {% endif %}
    </label>
    <input data-plugin="customTel"/ placeholder=" " >{{ children }}{# <input type="text" name="test input" id="input1" class="form-control"> #}
    {% if element['#errors'] %}
      <span class="error-text">{{ element['#errors'] }}</span>
    {% endif %}
    {% if element['#help'] %}
      <span class="form-help-text">{{ element['#help'] }}</span>
    {% endif %}
  </div><!--/form-item-->

